// function takes a number and then finds the factorial

function factorialize(num) {
  if (num === 0) {
    return 1;
  }
 else {
  
  // for loop starting at one less than num, decreasing each time
  for (var i = num - 1; i >= 1; i--) {
    num = num * i;
    
  }
  
}
  return num; 
}  

factorialize(5);

